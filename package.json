{
  "name": "Laboji",
  "description": "Add emoji on layer name like colorful labels",
  "license": "MIT",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Laboji",
    "manifest": "src/manifest.json",
    "main": "Laboji.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "repository" : {
    "type": "git",
    "url": "git+https://github.com/rezaaa/Laboji.git"
  },
  "author": "Mohammad Reza Mahmoudi <me@rezamahmoudi.ir>"
}